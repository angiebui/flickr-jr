<div class="container">
  <div class="errors">	
   <% if @errors %>
   	<p><%= @errors %></p>
   <% end %>
  </div>

  <h2> Upload Image </h2>
  <form action="/upload" method="post" enctype="multipart/form-data">
    <ul>
    <%= erb :_photo_upload, :layout => false %>
    <button id="plus-photo" type="button"> + </button><br><br>
    <input type="submit" value="Upload">
    </ul>
  </form>
</div>
